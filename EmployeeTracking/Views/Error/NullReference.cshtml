@model System.Web.Mvc.HandleErrorInfo
@{
    ViewBag.Title = "NullReference";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Error</h2>
<section class="content">
    <div class="row">
        <div class="col-md-12">
            <div class="box">
                <div class="box-body">
                    <div class="col-md-12">@Model.Exception.Message</div>
                    <div class="col-md-12">@Model.Exception.Source</div>
                </div>
            </div>
        </div>
    </div>
</section>
